{
  "name": "typescript-course",
  "version": "1.0.0",
  "description": "API Desenvolvida utilizando TypeScript, Node, Es6, Postgre e Sequelize",
  "main": "server/server.ts",
  "scripts": {
    "start": "cross-env port=5000 NODE_ENV=development ./node_modules/.bin/ts-node ./server/server.ts",
    "builded": "cross-env port=5000 NODE_ENV=development node ./dist/server/server.js",
    "unit-test": "cross-env NODE_ENV=test mocha --exit --opts mocha.opts ./dist/tests/unit/unit.test.js",
    "integration-test": "cross-env NODE_ENV=test mocha --exit --opts mocha.opts ./dist/tests/integration/integration.test.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thiagomartos/typescript-course.git"
  },
  "keywords": [
    "API",
    "Typescript",
    "Node",
    "ES6"
  ],
  "author": "Thiago Martos",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/thiagomartos/typescript-course/issues"
  },
  "homepage": "https://github.com/thiagomartos/typescript-course#readme",
  "dependencies": {
    "bluebird": "^3.5.3",
    "body-parser": "^1.18.3",
    "cross-env": "^5.2.0",
    "express": "^4.16.4",
    "http": "0.0.0",
    "http-status": "^1.3.1",
    "morgan": "^1.9.1",
    "pg": "^7.8.1",
    "sequelize": "^4.42.0",
    "sequelize-cli": "^5.4.0",
    "typescript": "^3.3.3333"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/chai": "^4.1.7",
    "@types/express": "^4.16.1",
    "@types/http-status": "^0.2.30",
    "@types/mocha": "^5.2.5",
    "@types/morgan": "^1.7.35",
    "@types/sequelize": "^4.27.37",
    "@types/supertest": "^2.0.7",
    "chai": "^4.2.0",
    "mocha": "^5.2.0",
    "sequelize": "^4.42.0",
    "supertest": "^3.4.2",
    "testdouble": "^3.11.0",
    "ts-node": "^8.0.2"
  }
